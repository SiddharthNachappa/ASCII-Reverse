#include<iostream>
using namespace std;

int main(){
    //Lets say we take in 3 integers from the user
    int a , b , c;
    cout<<"Enter the 3 numbers you want to convert to get their ASCII values";
    cin>> a>>b>>c;
    cout<<"The ASCII value of the charaters are : "<< char(a)<<" "<< char(b) <<" "<< char(c);
    
}
